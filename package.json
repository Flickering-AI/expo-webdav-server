{
  "name": "expo-webdav-server",
  "version": "0.1.0",
  "description": "Start a webdav server on Android",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "open -a \"Xcode\" example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "expo",
    "expo-webdav-server",
    "ExpoWebdavServer"
  ],
  "repository": "https://github.com/flickering-AI/expo-webdav-server",
  "bugs": {
    "url": "https://github.com/flickering-AI/expo-webdav-server/issues"
  },
  "author": "Xuanping <a845326948@gmail.com> (https://github.com/lxp-git)",
  "license": "MIT",
  "homepage": "https://github.com/flickering-AI/expo-webdav-server#readme",
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/react": "^18.0.25",
    "expo-module-scripts": "^3.4.0",
    "expo-modules-core": "^1.11.8",
    "release-it": "^17.0.3"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}",
      "requireCleanWorkingDir": false,
      "addFiles": [
        "package.json"
      ]
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  }
}
